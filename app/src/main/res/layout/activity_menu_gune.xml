<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#58718B"
    tools:context=".Activity.Menu_Gune_Activity">

    <Button
        android:id="@+id/btn_LoginAtzera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="228dp"
        android:backgroundTint="#092635"
        android:text="Button"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_gune_3" />

    <TextView
        android:id="@+id/lbl_menu_gune"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="Menu Guneak"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_gune_1"
        android:layout_width="241dp"
        android:layout_height="51dp"
        android:layout_marginTop="164dp"
        android:backgroundTint="#092635"
        android:text="Kontserba Fabrika"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_menu_gune" />

    <Button
        android:id="@+id/btn_gune_2"
        android:layout_width="241dp"
        android:layout_height="51dp"
        android:layout_marginTop="68dp"
        android:backgroundTint="#092635"
        android:text="Arrain saltzaileen eskulturak"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_gune_1" />

    <Button
        android:id="@+id/btn_gune_3"
        android:layout_width="241dp"
        android:layout_height="51dp"
        android:layout_marginTop="188dp"
        android:backgroundTint="#092635"
        android:text="Saregileen Murala"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_gune_1" />
</androidx.constraintlayout.widget.ConstraintLayout>